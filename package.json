{
  "name": "elyfi-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync ./build s3://elyfi-bucket --profile=hyuno",
    "aws-build": "aws cloudfront create-invalidation --distribution-id EH1U6Z7PEQO75 --paths \"/*\" --profile=hyuno",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "download-schema": "apollo service:download --endpoint=https://api.thegraph.com/subgraphs/name/modonguk/elyfi graphql-schema.json",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "apexcharts": "^3.27.1",
    "babel-loader": "8.1.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.2.0",
    "google-map-react": "^2.1.10",
    "i18next": "^20.2.1",
    "moment": "^2.29.1",
    "node-sass": "4.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "2",
    "react-i18next": "^11.8.12",
    "react-loading-skeleton": "^2.2.0",
    "react-native-responsive-linechart": "^5.7.1",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-spinners": "^0.11.0",
    "react-static-google-map": "^0.8.0",
    "sass-loader": "^11.0.1",
    "typescript": "^4.2.3",
    "web-vitals": "^1.0.1",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.20",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/google-map-react": "^2.1.1",
    "@types/graphql": "^14.5.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.5",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.7",
    "apollo": "^2.33.4"
  }
}
